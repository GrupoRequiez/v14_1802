<odoo>
  <data>
    <record id="view_stock_quant_tree_inherit_tree_view" model="ir.ui.view">
      <field name="name">view.stock.quant.tree.inherit.tree.view</field>
      <field name="model">stock.quant</field>
      <field name="inherit_id" ref="stock.view_stock_quant_tree"/>
      <field name="arch" type="xml">
        <xpath expr="//tree/field[@name='product_uom_id']" position="after">
          <field name="brand" attrs="{'readonly': True}" optional="hide"/>
          <field name="abc" attrs="{'readonly': True}" optional="hide"/>
          <field name="xyz" attrs="{'readonly': True}" optional="hide"/>
          <field name="line" attrs="{'readonly': True}" optional="hide"/>
          <field name="sellers" attrs="{'readonly': True}" optional="hide"/>
        </xpath>
      </field>
    </record>

